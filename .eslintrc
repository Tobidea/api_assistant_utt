{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "mocha": true
  },
  "rules": {
    "no-trailing-spaces":
      ["error",
      {
        "skipBlankLines": true,
        "ignoreComments": true
      }
      ],
      "no-console": 0
    }
}
